<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

  
  <mapper namespace="Calendar">
  
  <insert id="calWriteInsert" parameterType="Calendar">
  	INSERT INTO CALENDAR(member_id,calendar_title,calendar_content,calendar_rdate as calendar_rDate,calendar_wdate)
  	VALUES(#{member_id},#{calendar_title},#{calendar_content},#{calendar_rDate},SYSDATE)	
</insert>
 </mapper>